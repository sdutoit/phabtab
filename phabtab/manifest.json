{
  "name": "PhabTab",
  "version": "0.5.4",
  "description": "Chrome Tab for Phabricator",
  "permissions": [
    "alarms",
    "cookies",
    "storage"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ],
  "background": {
    "page": "ui/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_title": "PhabTab",
  	"default_popup": "ui/popup.html"
  },
  "options_ui": {
    "page": "ui/options.html",
    "open_in_tab": false
  },
  "icons": {
    "32": "ui/icons/review-blue-32.png",
    "48": "ui/icons/review-blue-48.png",
    "128": "ui/icons/review-blue-128.png"
  },
  "manifest_version": 2
}
